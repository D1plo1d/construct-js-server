{
  "name": "@tegapp/workspace",
  "version": "0.13.1",
  "private": true,
  "bin": {
    "teg": "./teg"
  },
  "scripts": {
    "lint": "eslint ./packages/",
    "delete-node-modules": "rm -rf node_modules && rm -rf packages/*/node_modules",
    "bootstrap": "sh ./scripts/bootstrap.sh",
    "bootstrap:minimal": "sh ./scripts/bootstrap-minimal.sh",
    "make-dirs": "sudo mkdir /run/log/teg || true; sudo mkdir /etc/teg || true; sudo mkdir /var/lib/teg || true; sudo chmod 700 /run/log/teg && sudo chown $USER: /run/log/teg && sudo chmod 700 /etc/teg && sudo chown $USER: /etc/teg && sudo chmod 700 /var/lib/teg && sudo chown $USER: /var/lib/teg",
    "install-all": "yarn --network-timeout 1000000 && lerna exec --concurrency 1 -- yarn --network-timeout 1000000 && cd ./packages/teg-web-ui/ && yarn --network-timeout 1000000",
    "install-all-fast": "yarn && lerna exec yarn && cd ./packages/teg-web-ui/ && yarn",
    "update-defs": "./scripts/updateDefs.sh",
    "frontend:build": "cd packages/teg-web-ui/ && yarn build",
    "test": "jest",
    "serve-http": "cd ./packages/teg-web-ui/ && yarn serve",
    "start:production-dat": "gulp start",
    "start": "sh ./scripts/start-tmux.sh",
    "start:web-ui": "cd ./packages/teg-web-ui/ && yarn start",
    "start:production": "NODE_ENV=production RUST_ENV=production DAT_URL=\"dat://d6531c1a9b6b23901fa4c766b5842d08fb498e9aabbeca31b14d75ba93dc4ab6/\" gulp start",
    "debug-host": "cd ./packages/teg-host-posix && yarn debug",
    "dev-host": "cd ./packages/teg-host-posix && yarn dev",
    "protobuf:build": "cd packages/teg-core/ && npx pbjs -t static-module -w commonjs -o src/printer/effects/protobufs.js ../../protos/MachineMessage.proto ../../protos/CombinatorMessage.proto",
    "tegmarlin:build:x64": "cd ./packages/teg-marlin && cargo build --release --bins && rsync -r ./target/release/teg-marlin ../../snap/teg-marlin-bin/x64",
    "tegmarlin:build:armv7": "PACKAGE=\"teg-marlin\" ./scripts/build-teg-auth-armv7.sh",
    "tegauth:build:x64": "cd ./packages/teg-auth && cargo build --release --bins && rsync -r ./target/release/teg-auth ../../snap/teg-auth-bin/x64 && rsync -r ./target/release/teg-add-invite ../../snap/teg-auth-bin/x64 && rsync -r ./target/release/teg-restore-backup ../../snap/teg-auth-bin/x64",
    "tegauth:build:armv7": "PACKAGE=\"teg-auth\" ./scripts/build-teg-auth-armv7.sh",
    "tegauth:scp": "yarn tegauth:build:armv7 && scp ./snap/teg-auth-bin/armv7/teg-auth $TEG_ARMV7_HOST:~/",
    "snap:build": "./scripts/build-snap.sh",
    "snap:upload": "snapcraft upload --release=beta ./dist/*_amd64.snap && snapcraft upload --release=beta ./dist/*_armhf.snap",
    "snap:remove": "sudo snap remove teg",
    "snap:install": "sudo snap install --devmode --dangerous ./dist/teg_$(echo $npm_package_version)_amd64.snap && yarn journalctl",
    "snap:install:remote": "ssh $TEG_ARMV7_HOST rm -f ./tegh_$(echo $npm_package_version)_armhf.snap; rsync ./dist/tegh_$(echo $npm_package_version)_armhf.snap $TEG_ARMV7_HOST:~/ && ssh $TEG_ARMV7_HOST sudo snap install --devmode --dangerous ./tegh_$(echo $npm_package_version)_armhf.snap && ssh $TEG_ARMV7_HOST -t 'sudo snap logs tegh.server -f -n=50'",
    "snap:build-and-install": "yarn snap:build && yarn snap:install",
    "snap:version": "./scripts/releaseConfirmation.sh && lerna version --conventional-commits",
    "snap:publish": "yarn snap:version && yarn snap:upload",
    "snap:debug": "sudo journalctl --output=short --follow --all | sudo snappy-debug",
    "journalctl": "journalctl -u snap.tegh.teg.service -n 100 -f",
    "journalctl:marlin": "journalctl -u snap.tegh.teg-marlin.service -n 100 -f",
    "version": "yarn snap:build"
  },
  "command": {
    "bootstrap": {
      "npmClient": "yarn",
      "useWorkspaces": true
    }
  },
  "engines": {
    "node": "12.11.1"
  },
  "os": [
    "linux"
  ],
  "workspaces": {
    "packages": [
      "packages/teg-auth",
      "packages/teg-core",
      "packages/teg-marlin",
      "packages/teg-macros-default",
      "packages/teg-host-posix",
      "packages/teg-schema"
    ],
    "nohoist": [
      "**/react",
      "**/react/**",
      "**/react-dom",
      "**/react-dom/**",
      "**/react-native",
      "**/react-native/**",
      "**/react-native-scripts",
      "**/react-native-scripts/**",
      "**/react-native-usbserial",
      "**/react-native-usbserial/**",
      "**/expo",
      "**/expo/**",
      "**/jest",
      "**/jest/**",
      "**/jest-expo",
      "**/jest-expo/**"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "chalk": "^2.4.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-immutablejs": "^0.1.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-monorepo": "^0.2.0",
    "eslint-plugin-react": "^7.11.0",
    "fast-async": "^6.3.8",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "jest": "^24.0.0-alpha.6",
    "jest-date-mock": "^1.0.6",
    "lerna": "^3.13.2",
    "netlify-cli": "^2.11.22",
    "plumber": "^0.4.8",
    "rimraf": "^2.6.1",
    "through2": "^2.0.3"
  },
  "resolutions": {
    "friendly-errors-webpack-plugin": "1.7.0"
  }
}
